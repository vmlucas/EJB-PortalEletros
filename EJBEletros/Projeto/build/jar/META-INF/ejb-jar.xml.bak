<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
  <enterprise-beans>
    <session>
      <description>Session Bean ( Stateless )</description>
      <display-name>UsuarioEJB</display-name>
      <ejb-name>UsuarioEJB</ejb-name>
      <home>EJB.Eletros.UsuarioEJBHome</home>
      <remote>EJB.Eletros.UsuarioEJB</remote>
      <ejb-class>EJB.Eletros.UsuarioEJBBean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
    <session>
      <description>Session Bean ( Stateless )</description>
      <display-name>FornecedorEJB</display-name>
      <ejb-name>FornecedorEJB</ejb-name>
      <home>EJB.Eletros.FornecedorEJBHome</home>
      <remote>EJB.Eletros.FornecedorEJB</remote>
      <ejb-class>EJB.Eletros.FornecedorEJBBean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
    <session>
      <description>Session Bean ( Stateless )</description>
      <display-name>ParticipanteEJB</display-name>
      <ejb-name>ParticipanteEJB</ejb-name>
      <home>EJB.Eletros.ParticipanteEJBHome</home>
      <remote>EJB.Eletros.ParticipanteEJB</remote>
      <ejb-class>EJB.Eletros.ParticipanteEJBBean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
  </enterprise-beans>
  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>UsuarioEJB</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>
